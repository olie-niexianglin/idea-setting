<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="18" />
    <option name="FONT_FAMILY" value="Consolas" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="JAVA" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="BIDI_TEXT_DIRECTION" value="LTR" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="\btodo-olie\b.*">
      <option name="FOREGROUND" value="ddded9" />
      <option name="BACKGROUND" value="de2b43" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
    </pattern>
    <filter name="todo-olie">
      <pattern index="2" />
    </filter>
  </component>
</application>