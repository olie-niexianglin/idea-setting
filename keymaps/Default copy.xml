<keymap version="1" name="Default copy" parent="$default">
  <action id="ActivateJSONFormatterToolWindow">
    <keyboard-shortcut first-keystroke="shift ctrl back_quote" />
  </action>
  <action id="FindInPath">
    <keyboard-shortcut first-keystroke="ctrl alt slash" />
  </action>
  <action id="FixDocComment">
    <keyboard-shortcut first-keystroke="alt m" />
  </action>
  <action id="ShowSettings">
    <keyboard-shortcut first-keystroke="ctrl alt 0" />
  </action>
</keymap>